<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    app:startDestination="@id/mainFragment2">
    <fragment
        android:id="@+id/mainFragment2"
        android:name="com.example.thirdproject.FragmentMain"
        android:label="MainFragment"
        tools:layout="@layout/fragment_main">
        <action
            android:id="@+id/action_mainFragment2_to_blankFragment1"
            app:destination="@id/blankFragment1" />
        <action
            android:id="@+id/action_mainFragment2_to_blankFragment2"
            app:destination="@id/blankFragment2" />
        <action
            android:id="@+id/action_mainFragment2_to_blankFragment3"
            app:destination="@id/blankFragment3" />
    </fragment>
    <fragment
        android:id="@+id/blankFragment1"
        android:name="com.example.thirdproject.BlankFragment1"
        android:label="fragment_blank1"
        tools:layout="@layout/fragment_blank1">
        <deepLink
            android:id="@+id/deepLink"
            app:uri="http://www.aboba.com/thirdProject" />
        <deepLink
            android:id="@+id/deepLink2"
            app:uri="http://www.aboba.com/thirdProject2/{text}/{desc}/{full_desc}" />
    </fragment>
    <fragment
        android:id="@+id/blankFragment2"
        android:name="com.example.thirdproject.BlankFragment2"
        android:label="fragment_blank2"
        tools:layout="@layout/fragment_blank2" />
    <fragment
        android:id="@+id/blankFragment3"
        android:name="com.example.thirdproject.BlankFragment3"
        android:label="fragment_blank3"
        tools:layout="@layout/fragment_blank3" />

</navigation>

